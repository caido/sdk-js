{
  "name": "@caido/sdk-client",
  "version": "0.1.2",
  "description": "Client SDK for interacting with a Caido instance",
  "author": "Caido Labs Inc. <dev@caido.io>",
  "repository": "https://github.com/caido/sdk-js/",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "generate": "pnpm generate:graphql && pnpm generate:rest",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:rest": "openapi-ts"
  },
  "dependencies": {
    "@caido/server-auth": "0.2.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@urql/core": "^6.0.0",
    "@urql/exchange-auth": "^3.0.0",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.0",
    "zod": "^4.3.0",
    "wonka": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typed-document-node": "5.0.11",
    "@graphql-codegen/typescript": "4.1.1",
    "@graphql-codegen/typescript-operations": "4.3.1",
    "@hey-api/openapi-ts": "0.76.0",
    "typescript": "5.6.2"
  }
}
