{
  "name": "@caido/quickjs-types",
  "version": "0.6.0",
  "description": "Typing for the QuickJS Engine",
  "author": "Caido Labs Inc. <dev@caido.io>",
  "license": "MIT",
  "types": "./src/index.d.ts",
  "files": [
    "src/*"
  ],
  "scripts": {
    "typing:llrt": "rsync -av --exclude=node_modules --include='*.d.ts' --include='*/' --exclude='*' ../../../dependency-llrt/types/ ./src/llrt/",
    "typing:extra": "rsync -av --exclude=node_modules --include='*.d.ts' --include='*/' --exclude='*' ../../../rquickjs-extra/types/ ./src/extra/"
  },
  "dependencies": {
    "prettier": "3.3.2"
  }
}
